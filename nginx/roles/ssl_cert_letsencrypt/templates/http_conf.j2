server {
        listen {{nginx_http_port}} ;

        root {{nginx_root_path}}/{{item}} ;
        {% for file in nginx_start_file %}
        {{file}}{% endfor %};
        server_name {{item}}.{{nginx_dns_name}} ;

        location /.well-known {
          allow all;

}
 
